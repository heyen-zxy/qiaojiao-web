<div class="modal-header">
  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
    <span aria-hidden="true">×</span></button>
  <h4 class="modal-title">提现</h4>
</div>

<div class="modal-body">
  <%= render partial: 'admin/server_users/form', locals: {log: @log} %>
</div>
<div class="modal-footer">
  <button type="button" class="btn btn-default pull-left" data-dismiss="modal">关闭</button>
  <button type="button" class="btn btn-primary set-admin">确认</button>
</div>

<script type=text/javascript>
  $(function(){
    $('select').select2();
    $('.set-admin').click(function(){
      showSpinner()
      $.ajax({
        type: 'POST',
        url: '<%= cash_admin_server_user_path @log.admin %>',
        data: {commission: $('#admin_commission_log_commission').val(), desc: $('#admin_commission_log_desc').val()},
        dataType: 'script',
        success: function (data) {

        }
      })
    })
  })
</script>

