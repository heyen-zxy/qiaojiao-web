<%= simple_form_for  [:admin, log], class: 'forms-sample', onkeydown: "if(event.keyCode==13)return false;"  do |f| %>
  <div class="box-body">
    <div class="row">
      <div class="col-md-12 form-group">
        <%= f.input :commission, label: '调整金额(扣除的话输入负数，例：-100)',input_html: { value: log.view_commission.abs} %>
      </div>
      <div class="col-md-12 form-group">
        <%= f.input :desc %>
      </div>
    </div>
  </div>
<% end %>